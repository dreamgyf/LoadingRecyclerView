<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="LoadingRecyclerView">
		<attr name="enableLoad" format="boolean"/>
		<attr name="direction" format="flags">
			<flag name="start" value="0x01"/>
			<flag name="end" value="0x02"/>
		</attr>
		<attr name="loadingBackgroundColor" format="color"/>
		<attr name="loadingArcColor" format="color"/>
	</declare-styleable>
</resources>